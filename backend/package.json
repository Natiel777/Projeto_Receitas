{
  "name": "suareceita-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "build": "npx prisma migrate deploy",
    "test:create-db": "dotenv -e .env.test -- prisma db push --accept-data-loss",
    "test": "npm run test:create-db && dotenv -e .env.test -- jest --runInBand",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3"
  }
}
